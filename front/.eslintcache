 <td className=\"hash\"><div>{users[0].num}</div></td>\n                        <td title={users[0].userEmail} className=\"long-td-fixed\"><div>{users[0].userEmail}</div></td>\n                        <td title={users[0].userDomain} className=\"long-td-fixed\"><div>{users[0].userDomain}</div></td>\n                        <td className=\"lisence\"><UserLicense user={users[0]} onSelect={onLicenseSelect} /></td>\n                        <td><div>{userService.getFormatDate(users[0].registerDate)}</div></td>\n                        <td><div><UserExpirationDate user={users[0]} onSelect={onDateSelect} /></div></td>\n                        <td className=\"delete-user\" onClick={() => onDeleteUser(users[0]._id)}><section>X</section></td>\n                    </tr>\n                </tbody> */}\n            </table>}\n            <Table striped bordered hover size=\"sm\" >\n                <thead ref={tHeadRef}>\n                    <tr className=\"reg-tr\">\n                        {/* <th className=\"user-id\">ID</th> */}\n                        <th onClick={() => onSetSortBy('registerDate')} className=\"hash\">#</th>\n                        <th onClick={() => onSetSortBy('userEmail')} className=\"long-td\">Email</th>\n                        <th onClick={() => onSetSortBy('userDomain')} className=\"long-td\">Domain</th>\n                        <th>License</th>\n                        <th onClick={() => onSetSortBy('registerDate')}>Registered Date</th>\n                        <th onClick={() => onSetSortBy('expirationDate')}>Expiration Date</th>\n                        <th className=\"delete-user-th\"></th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {users.map(user => {\n                        return <tr className={userLicenseClass(user)} key={user._id}>\n                            <td className=\"hash\"><div>{user.num}</div></td>\n                            <td title={user.userEmail} className=\"long-td\"><div>{user.userEmail}</div></td>\n                            <td title={user.userDomain} className=\"long-td\"><div>{user.userDomain}</div></td>\n                            <td className=\"lisence\"><UserLicense user={user} onSelect={onLicenseSelect} /></td>\n                            <td><div>{userService.getFormatDate(user.registerDate)}</div></td>\n                            <td><div><UserExpirationDate user={user} onSelect={onDateSelect} /></div></td>\n                            <td className=\"delete-user\" onClick={() => onDeleteUser(user._id)}><section>X</section></td>\n                        </tr>\n                        // return <tr key={user._id}>\n                        //     <td className=\"hash\">{user.num}</td>\n                        //     <td title={user.userEmail} className=\"long-td\">{user.userEmail}</td>\n                        //     <td title={user.userDomain} className=\"long-td\">{user.userDomain}</td>\n                        //     <td><UserLicense user={user} onSelect={onLicenseSelect} /></td>\n                        //     <td>{new Date(user.registerDate).toLocaleDateString()}</td>\n                        //     <td><UserExpirationDate user={user} onSelect={onDateSelect} /></td>\n                        // </tr>\n                    })}\n                </tbody>\n            </Table>\n        </>\n    )\n\n\n\n}\n\nexport default UserList;","/Users/stavyaarbar/Desktop/dev/esl-projects/esl-admin-portal/frontend/src/services/userService.js",[],"/Users/stavyaarbar/Desktop/dev/esl-projects/esl-admin-portal/frontend/src/cmps/Filter.js",[],"/Users/stavyaarbar/Desktop/dev/esl-projects/esl-admin-portal/frontend/src/services/sessionStorageService.js",[],"/Users/stavyaarbar/Desktop/dev/esl-projects/esl-admin-portal/frontend/src/cmps/UserLicense.js",[],"/Users/stavyaarbar/Desktop/dev/esl-projects/esl-admin-portal/frontend/src/cmps/UserExpirationDate.js",[],"/Users/stavyaarbar/Desktop/dev/esl-projects/esl-admin-portal/frontend/src/ser